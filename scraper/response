            <script>
            (function _jqIsReady() {
                if (typeof $ === "undefined") {
                    setTimeout(_jqIsReady, 10);
                    return;
                }
                
                // var lastParagraph = $("#ignorediv.Art-exp_wr p").not(".SoFl_wp p, .tgs p, div:has([src*='vdo.ai']) p, p:has(iframe)").last();
                var lastParagraphText = '';
                var lastParagraph = 0;
                var articleDiscription = document.querySelectorAll("#ignorediv.Art-exp_wr p");
                var objLength = Object.keys(articleDiscription).length;
                var noElfound = true;
                do {
                    objLength--;
                    if (objLength == 0 || articleDiscription[objLength].innerHTML.length > 10) {
                        if (!articleDiscription[objLength].getElementsByTagName("iframe").length) {
                            noElfound == false;
                            lastParagraphText = articleDiscription[objLength].innerHTML;
                            lastParagraph = objLength;
                            break;
                        }
                    }else{
                        articleDiscription[objLength].remove();
                    }
                } while (noElfound);
                var newElement = document.createElement("div");
